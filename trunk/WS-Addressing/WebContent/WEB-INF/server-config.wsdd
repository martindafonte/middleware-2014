  <ns1:parameter name="enableRemoteAdmin" value="false"/>
  <ns1:parameter name="className" value="org.apache.axis.utils.Admin"/>
  <ns1:namespace>http://xml.apache.org/axis/wsdd/</ns1:namespace>
 </ns1:service>
 <ns1:service name="AddressingSender" provider="java:RPC" style="wrapped" use="literal">
  <ns2:operation name="returnString" qname="ns1:returnString" returnQName="ns1:returnStringReturn" returnType="xsd:string" soapAction="" xmlns:ns1="http://data" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns2="http://xml.apache.org/axis/wsdd/">
   <ns2:parameter qname="ns1:parm1" type="xsd:string"/>
  </ns2:operation>
  <ns1:parameter name="allowedMethods" value="returnString"/>
  <ns1:parameter name="typeMappingVersion" value="1.2"/>
  <ns1:parameter name="wsdlPortType" value="AddressingSender"/>
  <ns1:parameter name="className" value="data.AddressingSender"/>
  <ns1:parameter name="wsdlServicePort" value="AddressingSender"/>
  <ns1:parameter name="schemaQualified" value="http://data"/>
  <ns1:parameter name="wsdlTargetNamespace" value="http://data"/>
  <ns1:parameter name="wsdlServiceElement" value="AddressingSenderService"/>
